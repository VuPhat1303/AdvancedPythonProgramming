print ( 'xin chào' . upper ())
print ( 'Chúc mừng sinh nhật!' . low ())
print ( 'WeeeEEEEeeeEEEEeee' . swapcase ())
print ( 'ABC123' . isupper ())
print ( 'aeiouAEIOU' . count ( 'a' ))
print ( 'xin chào' . endwith ( 'o' ))
print ( 'xin chào' . startwith ( 'H' ))
print ( 'Xin chào {0}' . format ( 'Python' ))
print ( 'Xin chào {0}! Xin chào {1}!' . format ( 'Python' , 'World' ))
